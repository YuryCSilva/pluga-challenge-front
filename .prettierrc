{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "jsxSingleQuote": true,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-classnames",
        "prettier-plugin-merge"
    ],
    "tailwindStylesheet": "./src/shared/styles/globals.css",
    "endingPosition": "absolute",
    "customFunctions": [
        "clsx",
        "twMerge",
        "cn"
    ]
}